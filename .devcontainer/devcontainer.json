{
  "name": "silverorange-php-dev-test",
  "dockerComposeFile": [
    "../docker-compose.yaml"
  ],
  "service": "php",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
        "php-cs-fixer.onsave": true
      },
      "extensions": [
        "felixfbecker.php-debug",
        "bmewburn.vscode-intelephense-client",
        "junstyle.php-cs-fixer"
      ]
    }
  },
  "forwardPorts": [
    8000
  ]
}
